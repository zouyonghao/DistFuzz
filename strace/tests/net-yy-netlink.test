. "${srcdir=.}/init.sh"

check_prog sed

opts="${*:--yy}"

run_prog ../netlink_netlink_diag
run_prog > /dev/null
run_strace -a22 ${opts} -eclose,network $args > "$EXP"

# Filter out close() calls made by ld.so and libc.
sed -n '/socket/,$p' < "$LOG" > "$OUT"
match_diff "$OUT" "$EXP"
