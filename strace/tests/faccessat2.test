. "${srcdir=.}/init.sh"

check_prog sed

run_prog > /dev/null
run_strace -a27 --trace=faccessat2 "$@" $args > "$EXP"

# Filter out faccessat2() calls made by ld.so and libc.
sed -n '/^faccessat2(-1, NULL,/,$p' < "$LOG" > "$OUT"
match_diff "$OUT" "$EXP"
