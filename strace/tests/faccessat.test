. "${srcdir=.}/init.sh"

check_prog sed

run_prog > /dev/null
run_strace -a23 --trace=faccessat "$@" $args > "$EXP"

# Filter out faccessat() calls made by ld.so and libc.
sed -n '/faccessat(-1, NULL,/,$p' < "$LOG" > "$OUT"
match_diff "$OUT" "$EXP"
