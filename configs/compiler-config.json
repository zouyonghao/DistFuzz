{
    "instrumentors": [
        "DefaultInstrumentor"
    ],
    "additional_link_args": [
        "/tmp/default_instrument_lib.c.o",
        "-fsanitize=address"
    ],
    "additional_compile_args": [
        "-g",
        "-fPIC",
        "-fsanitize=address"
    ],
    "ignored_args": [
        "-O1",
        "-O2",
        "-O3",
        "-gstabs",
        "-mincoming-stack-boundary",
        "-mpreferred-stack-boundary",
        "-Wcast-align",
        "-Wconstant-conversion",
        "-Werror"
    ]
}