{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/llvm-11",
                "/usr/include/x86_64-linux-gnu",
                "${workspaceFolder}/build/bpf/bpftool/bootstrap/libbpf/include",
                "${workspaceFolder}/build/bpf/bpftool/bootstrap/libbpf/include",
                "${workspaceFolder}/build/bpf/libbpf"
            ],
            "defines": [
                "__USE_GNU",
                "ENABLE_STACKTRACE"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "linux-clang-x64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}